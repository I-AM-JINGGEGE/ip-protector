<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="#ff01d689">
    <item>
        <shape android:shape="rectangle">
            <gradient android:type="linear"
                android:useLevel="true"
                android:startColor="#02EDBE"
                android:endColor="#01D689"
                android:angle="315" />
            <corners android:radius="4dp" />
            <item android:name="android:shadowColor">#61C26547</item>
            <item android:name="android:shadowDx">0</item>
            <item android:name="android:shadowDy">2</item>
        </shape>
    </item>
</ripple>